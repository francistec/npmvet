{
  "name": "@francis.tec/npmvet",
  "version": "0.1.6",
  "description": "A simple CLI tool for vetting npm package versions",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "run": "node dist/index.js",
    "br": "npm run build && npm run run",
    "prepublish": "npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:harksys/npmvet.git"
  },
  "author": "Andrew Hathaway <andrew@harksys.com>",
  "license": "MIT",
  "bin": {
    "npmvet": "./dist/index.js"
  },
  "jest": {
    "transform": {
      ".*": "./test/preprocessor.js"
    },
    "unmockedModulePathPatterns": [
      "lodash",
      "semver"
    ],
    "resetModules": true,
    "modulePaths": [
      "<rootDir>/src/"
    ],
    "testPathDirs": [
      "./test/tests"
    ],
    "testRegex": "^.*.(js)$",
    "globals": {
      "SRC_DIRECTORY": "<rootDir>/src/"
    },
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "collectCoverage": true
  },
  "devDependencies": {
    "@types/blessed": "^0.1.4",
    "@types/chalk": "^0.4.31",
    "@types/commander": "^2.3.31",
    "@types/lodash": "^4.14.52",
    "@types/node": "^7.0.5",
    "@types/semver": "^5.3.30",
    "@types/table": "^4.0.1",
    "jest": "^18.1.0",
    "typescript": "^2.1.5"
  },
  "dependencies": {
    "blessed": "^0.1.81",
    "chalk": "^1.1.3",
    "commander": "2.9.0",
    "lodash": "^4.17.4",
    "pluralize": "^3.1.0",
    "semver": "^5.3.0",
    "table": "^4.0.1"
  }
}
